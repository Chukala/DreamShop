<!doctype html>

<html lang="en">

<head>
  <meta charset="utf-8">
  <title><%=title%></title>
  <meta name="description" content="The HTML5 Herald">
  <meta name="author" content="SitePoint">
  <meta name="viewport" content="initial-scale=1.0, width=device-width">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"> 
  <link rel="stylesheet" href="../stylesheets/productstyle.css">
  <link rel="stylesheet" href="../stylesheets/adminstyle.css">

</head>

<body>
<%- include('admin/partials/adminmenu') %>

      <div class="prd_container">
          <div class="prd_tit">
            <h3>Our products</h3>
          </div>
          <div class="prd_menu">
            <ul class="menu_ul">
              <li class="prd_li"><a href="/products/?cat=dresses">Dresses</a></li>
              <li class="prd_li"><a href="/products/?cat=shirts">Shirts</a></li>
              <li class="prd_li"><a href="/products/?cat=shoes">Shoes</a></li>
              <li class="prd_li">Search:<input id="input"></input></li>
            </ul>		
          </div>
      </div>
          
      
      
      <div id="productlisting">
          <%for(let i = 0; i < productlist.length ; i++){ %>
            <%- include('products/products', {product:productlist[i]}) %>
          <%}%>
      </div>
      

    

  
<script src="../javascripts/custom.js"></script> 
<script src="../javascripts/checkout.js"></script>
<script type="text/javascript" src="https://js.stripe.com/v2/"></script>
